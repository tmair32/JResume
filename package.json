    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unocss": "^0.48.4",
    "vite": "^4.0.0",
